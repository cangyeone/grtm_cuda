void cplx_mmc_4(cplx a[4][4], cplx b[4][4])
{
    int i,j,k;
    int n = 4; 
    cplx c[4][4]; 
    register cplx temp;
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            c[i][j] = 0.;
        }
    }
    for(k=0;k<n;k++)
    {
        for(i=0;i<n;i++)
        {
            temp=a[i][k];
            for(j=0;j<n;j++)
            {
                c[i][j]+=temp*b[k][j];
            }
        }
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            b[i][j] = c[i][j];
        }
    }
}



